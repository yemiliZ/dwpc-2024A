{
  "name": "dwpc-2024a",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./dist/bin/www",
    "dev": "NODE_ENV=developement babel-watch --watch ./server/views ./server/bin/www",
    "devc": "NODE_ENV=developement webpack serve --config ./webpack.dev.config.js --mode development",
    "dev-nodemon": "NODE_ENV=developement nodemon --exec babel-node ./server/bin/www",
    "cleans": "rm -rf dist",
    "cleanc": "rm -f ./public/bundle.js",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "npm run b:c && npm run b:s"
  },
  "dependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-watch": "^7.8.1",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.36.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-hot-middleware": "^2.25.4"
  },
  "devDependencies": {
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-minify": "^0.5.2",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "nodemon": "^3.1.0",
    "webpack": "^5.90.3"
  }
}
